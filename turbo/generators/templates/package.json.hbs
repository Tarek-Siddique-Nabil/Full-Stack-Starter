{
  "name": "@repo/{{ name }}",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "clean": "git clean -xdf .cache .turbo dist node_modules",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false"
  }{{#if dependencies}},
  "dependencies": {
{{#each (split dependencies ",")}}    "{{trim this}}": "latest"{{#unless @last}},{{/unless}}
{{/each}}  }{{/if}},
  "devDependencies": {
    "@repo/typescript-config": "workspace:*"{{#if devDependencies}},
{{#each (split devDependencies ",")}}    "{{trim this}}": "latest"{{#unless @last}},{{/unless}}
{{/each}}{{/if}}
  }
}